{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["POST","GET"]
    }
  ],
  "functions": {
    "controllers/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "route/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "index.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}